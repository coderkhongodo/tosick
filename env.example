# Local Development Environment Variables (.env.local)
# WARNING: This file contains admin credentials - ONLY for localhost!
# DO NOT deploy this to production or commit to git!

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# MongoDB URI (Local development database)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/toeX

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# ADMIN CREDENTIALS - LOCALHOST ONLY!
# These credentials only work on localhost for security
# DO NOT commit these to git - set them in .env.local only
ADMIN_EMAIL=your_admin_email_here
ADMIN_PASSWORD=your_admin_password_here

# SECURITY NOTE:
# - Admin panel is only accessible on localhost
# - Production deployment will NOT have admin access
# - Keep these credentials secure and never commit to git
