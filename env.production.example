# Production Environment Variables (for Vercel deployment)
# Copy this to .env.production.local for local testing

# Firebase Configuration (Public keys only)
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# MongoDB URI (Production database)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/toeX

# JWT Secret (for session management)
JWT_SECRET=your_jwt_secret_here

# Note: Admin credentials are NOT included in production
# Admin panel only works on localhost with .env.local
